{
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "react-app",
    "rules" : {
      "jsx-a11y/href-no-hash": [0], 
      "react/jsx-filename-extension" : [ 1, { "extensions": [ ".js", ".jsx" ] } ],
      "jsx-a11y/no-noninteractive-element-interactions" : 0,
      "jsx-a11y/click-events-have-key-events" : 0,
      "react/prop-types" : 0,
      "linebreak-style": 0
    }
}
